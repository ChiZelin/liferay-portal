<cache2k>
<!-- An configuration example for the documentation -->

<version>1.0</version>
<defaultManagerName>SINGLE_VM_PORTAL_CACHE_MANAGER</defaultManagerName>
<ignoreMissingCacheConfiguration>true</ignoreMissingCacheConfiguration>
<ignoreAnonymousCache>true</ignoreAnonymousCache>
<defaults>
	<cache>
	  <eternal>false</eternal>
	  <entryCapacity>10000</entryCapacity>
	  <expireAfterWrite>10m</expireAfterWrite>
	</cache>
</defaults>
<caches>
	<cache>
	  <name>com.liferay.portal.security.exportimport.UserImporter</name>
	  <eternal>false</eternal>
	  <entryCapacity>10000</entryCapacity>
	  <expireAfterWrite>10m</expireAfterWrite>
	</cache>
	<cache>
	  <name>com.liferay.portal.servlet.ComboServlet</name>
	  <eternal>false</eternal>
	  <entryCapacity>1000</entryCapacity>
	  <expireAfterWrite>10m</expireAfterWrite>
	</cache>
	<cache>
	  <name>com.liferay.portal.servlet.ComboServlet$FileContentBag</name>
	  <eternal>false</eternal>
	  <entryCapacity>1000</entryCapacity>
	  <expireAfterWrite>10m</expireAfterWrite>
	</cache>
	<cache>
	  <name>com.liferay.portal.util.WebCachePool</name>
	  <eternal>false</eternal>
	  <entryCapacity>1000</entryCapacity>
	  <expireAfterWrite>10m</expireAfterWrite>
	</cache>
</caches>
</cache2k>